{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "headers": {
      "/(.*)": {
        "Cache-Control": "max-age=0, s-maxage=86400"
      }
    }
  }